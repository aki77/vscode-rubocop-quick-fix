{
  "name": "rubocop-quick-fix",
  "displayName": "Rubocop Quick Fix",
  "description": "Quick fix for Rubocop warnings",
  "version": "0.3.0",
  "publisher": "aki77",
  "repository": {
    "type": "git",
    "url": "https://github.com/aki77/vscode-rubocop-quick-fix.git"
  },
  "bugs": {
    "url": "https://github.com/aki77/vscode-rubocop-quick-fix/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "typescript": "^5.2.2",
    "vscode-test": "^1.5.0"
  },
  "extensionDependencies": []
}
