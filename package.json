{
  "name": "rubocop-quick-fix",
  "displayName": "Rubocop Quick Fix",
  "description": "Quick fix for Rubocop warnings",
  "version": "0.2.0",
  "publisher": "aki77",
  "repository": {
    "type": "git",
    "url": "https://github.com/aki77/vscode-rubocop-quick-fix.git"
  },
  "bugs": {
    "url": "https://github.com/aki77/vscode-rubocop-quick-fix/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "extensionDependencies": []
}
